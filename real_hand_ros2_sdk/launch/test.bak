from launch import LaunchDescription
from launch_ros.actions import Node

def generate_launch_description():
    return LaunchDescription([
        Node(
            package='real_hand_ros2_sdk',
            executable='real_hand_sdk',
            name='real_hand_sdk_left',
            output='screen',
            parameters=[{
                'hand_type': 'left',
                'hand_joint': "L10", # Change to the actual Real Hand model: L7, L10, L20, L21, L25
                'is_touch': True, # Whether it has pressure sensors
                'can': 'can0', # Change to the actual CAN bus name
            }],
        ),

        Node(
            package='real_hand_ros2_sdk',
            executable='real_hand_sdk',
            name='real_hand_sdk_right',
            output='screen',
            parameters=[{
                'hand_type': 'right',
                'hand_joint': "L10", # Change to the actual Real Hand model: L7, L10, L20, L21, L25
                'is_touch': True, # Whether it has pressure sensors
                'can': 'can0', # Change to the actual CAN bus name
            }],
        ),
    ])
